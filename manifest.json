{
  "manifest_version": 3,
  "name": "AtlasQuest",
  "version": "1.0",
  "description": "Shows population and GDP of cities on Google Maps.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://www.google.com/maps/*"],
  "content_scripts": [
    {
      "matches": ["https://www.google.com/maps/*"],
      "js": ["content.js"]
    }
  ]
}
